@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
    var content = (ContentModels.TeamMemberItem)Model.Content;

}

<div class="team-member-card">
        <img class="team-member-image" src="@content.TeamMemberImage.Url()" />
    <div class="team-member-content">
        <p class="team-member-name">@content.TeamMemberName</p>
        <p class="team-member-role">@content.TeamMemberRole</p>
    </div>
</div>