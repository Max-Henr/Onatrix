{
  "name": "Onatrix Rte",
  "alias": "Onatrix.Rte",
  "extensions": [
    {
      "type": "tiptapToolbarExtension",
      "kind": "styleMenu",
      "alias": "Onatrix.Tiptap.StyleMenu",
      "name": "Onatrix Tiptap Style Menu",
      "meta": {
        "alias": "OnatrixStyleMenu",
        "icon": "icon-palette",
        "label": "Onatrix styles"
      },
      "items": [
        {
          "label": "Headings",
          "items": [
            {
              "label": "Heading 2",
              "data": { "tag": "h2" }
            },
            {
              "label": "Heading 3",
              "data": { "tag": "h3" }
            },
            {
              "label": "Heading 4",
              "data": { "tag": "h4" }
            },
            {
              "label": "Heading 5",
              "data": { "tag": "h5" }
            },
            {
              "label": "Heading 6",
              "data": { "tag": "h6" }
            }
          ]
        },
        {
          "label": "Bullets",
          "data": {
            "tag": "ul",
            "class": "rte-ul"
          }
        }
      ]
    },
    {
      "type": "tiptapToolbarExtension",
      "kind": "styleMenu",
      "alias": "Onatrix.Tiptap.BrandColors",
      "name": "Onatrix Brand Colors",
      "meta": {
        "alias": "OnatrixBrandColors",
        "label": "Onatrix Colors"
      },
      "items": [
        {
          "label": "Text Colors",
          "items": [
            {
              "label": "Primary",
              "data": {
                "class": "text-primary"
              },
              "appearance": { "style": "color: #535656" }
            },
            {
              "label": "Sub 1",
              "data": {
                "class": "text-sub-1"
              },
              "appearance": { "style": "color: #f2eddc" }
            },
            {
              "label": "Sub 3",
              "data": {
                "class": "text-sub-3"
              },
              "appearance": { "style": "color: #d9c3a9" }
            }
          ]
        },
        {
          "label": "Backgrounds",
          "items": [
            {
              "label": "BG Default",
              "data": {
                "tag": "span",
                "style": "background-color: var(--bg-color); color: var(--text-color); padding: 0.125rem 0.375rem; border-radius: 4px;"
              },
              "appearance": { "style": "background: var(--bg-color); color: var(--text-color); border-radius: 4px; padding: 0 6px;" }
            },
            {
              "label": "Primary BG",
              "data": {
                "tag": "span",
                "style": "background-color: var(--primary-color); color: var(--white-text); padding: 0.125rem 0.375rem; border-radius: 4px;"
              },
              "appearance": { "style": "background: var(--primary-color); color: var(--white-text); border-radius: 4px; padding: 0 6px;" }
            },
            {
              "label": "Sub 1 BG",
              "data": {
                "tag": "span",
                "style": "background-color: var(--sub-color-1); color: var(--footer-bg-color); padding: 0.125rem 0.375rem; border-radius: 4px;"
              },
              "appearance": { "style": "background: var(--sub-color-1); color: var(--footer-bg-color); border-radius: 4px; padding: 0 6px;" }
            },
            {
              "label": "Sub 3 BG",
              "data": {
                "tag": "span",
                "style": "background-color: var(--sub-color-3); color: var(--footer-bg-color); padding: 0.125rem 0.375rem; border-radius: 4px;"
              },
              "appearance": { "style": "background: var(--sub-color-3); color: var(--footer-bg-color); border-radius: 4px; padding: 0 6px;" }
            },
            {
              "label": "Footer BG",
              "data": {
                "tag": "span",
                "style": "background-color: var(--footer-bg-color); color: var(--white-text); padding: 0.125rem 0.375rem; border-radius: 4px;"
              },
              "appearance": { "style": "background: var(--footer-bg-color); color: var(--white-text); border-radius: 4px; padding: 0 6px;" }
            }
          ]
        },
        {
          "label": "Typography",
          "items": [
            {
              "label": "Poppins",
              "data": {
                "tag": "span",
                "style": "font-family: var(--font-family);"
              },
              "appearance": { "style": "font-family: var(--font-family);" }
            },
            {
              "label": "Thin",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-thin);"
              }
            },
            {
              "label": "Extra Light",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-extralight);"
              }
            },
            {
              "label": "Light",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-light);"
              }
            },
            {
              "label": "Normal",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-normal);"
              }
            },
            {
              "label": "Medium",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-medium);"
              }
            },
            {
              "label": "Semi Bold",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-semi-bold);"
              }
            },
            {
              "label": "Bold",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-bold);"
              }
            },
            {
              "label": "Extra Bold",
              "data": {
                "tag": "span",
                "style": "font-weight: var(--font-extra-bold);"
              }
            }
          ]
        }
      ]
    }
  ]
}